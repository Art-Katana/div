							<h4 class="title"><%=post_title%></h4>
						<% } %>
						<div id="comm-id-<%=id%>">
							<%=comment%>
						</div>
						<% if (!$('#main').hasClass('lastcomments') && !$('#main').hasClass('social_profile')) {%>
							<div class="replay-block">
								<span class="replay icon-reviewsNeutral" data-post-id="<%=post_id%>" data-id="<%=id%>">Ответить</span>
							</div>
						<% } %>
					</div>
				</div>
			</div>
		</script>
		
